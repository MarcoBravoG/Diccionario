<?xml version="1.0" encoding="utf-8"?><android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:background="#efefef"    tools:context=".SeleccionDiccionario"    tools:layout_editor_absoluteY="25dp">    <Button        android:id="@+id/butD"        android:layout_width="50dp"        android:layout_height="50dp"        android:layout_marginBottom="8dp"        android:layout_marginEnd="100dp"        android:layout_marginLeft="8dp"        android:layout_marginRight="100dp"        android:layout_marginStart="8dp"        android:layout_marginTop="8dp"        android:background="@drawable/butdiccionlet"        android:fontFamily="sans-serif-thin"        android:onClick="d"        android:text='"D"'        android:textColor="#FFFFFF"        android:textStyle="bold"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toEndOf="@+id/butC"        app:layout_constraintTop_toBottomOf="@+id/textView5" />    <Button        android:id="@+id/butC"        android:layout_width="50dp"        android:layout_height="50dp"        android:layout_marginEnd="8dp"        android:layout_marginLeft="8dp"        android:layout_marginRight="8dp"        android:layout_marginStart="8dp"        android:background="@drawable/butdiccionlet"        android:fontFamily="sans-serif-thin"        android:onClick="c"        android:text='"C"'        android:textColor="#FFFFFF"        android:textStyle="bold"        app:layout_constraintBottom_toBottomOf="@+id/butD"        app:layout_constraintEnd_toStartOf="@+id/butD"        app:layout_constraintStart_toEndOf="@+id/ButB"        app:layout_constraintTop_toTopOf="@+id/butD" />    <Button        android:id="@+id/ButA"        android:layout_width="50dp"        android:layout_height="48dp"        android:layout_marginEnd="8dp"        android:layout_marginLeft="85dp"        android:layout_marginRight="8dp"        android:layout_marginStart="85dp"        android:background="@drawable/butdiccionlet"        android:fontFamily="sans-serif-thin"        android:onClick="a"        android:text='"A"'        android:textColor="#FFFFFF"        android:textStyle="bold"        app:layout_constraintBottom_toBottomOf="@+id/ButB"        app:layout_constraintEnd_toStartOf="@+id/ButB"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="@+id/ButB" />    <ImageView        android:id="@+id/imageView"        android:layout_width="295dp"        android:layout_height="194dp"        android:layout_marginEnd="8dp"        android:layout_marginLeft="8dp"        android:layout_marginRight="8dp"        android:layout_marginStart="8dp"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:srcCompat="@drawable/nuevo_lienzo_sf" />    <ImageView        android:id="@+id/imageView2"        android:layout_width="0dp"        android:layout_height="109dp"        android:adjustViewBounds="false"        android:background="#E2F1EE"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent"        app:srcCompat="@drawable/indeapp" />    <Button        android:id="@+id/ButB"        android:layout_width="50dp"        android:layout_height="50dp"        android:layout_marginEnd="8dp"        android:layout_marginLeft="8dp"        android:layout_marginRight="8dp"        android:layout_marginStart="8dp"        android:background="@drawable/butdiccionlet"        android:fontFamily="sans-serif-thin"        android:onClick="b"        android:text='"B"'        android:textColor="#FFFFFF"        android:textStyle="bold"        app:layout_constraintBottom_toBottomOf="@+id/butC"        app:layout_constraintEnd_toStartOf="@+id/butC"        app:layout_constraintStart_toEndOf="@+id/ButA"        app:layout_constraintTop_toTopOf="@+id/butC" />    <TextView        android:id="@+id/textView5"        android:layout_width="0dp"        android:layout_height="wrap_content"        android:layout_marginBottom="8dp"        android:background="#E2F1EE"        android:gravity="center"        android:text="Selecciona una letra"        android:textAlignment="center"        android:textColor="#2d2d2d"        android:fontFamily="sans-serif-thin"        android:textSize="@dimen/_27sdp"        app:layout_constraintBottom_toBottomOf="@+id/imageView"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintHorizontal_bias="0.0"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/imageView2"        app:layout_constraintVertical_bias="0.0" /></android.support.constraint.ConstraintLayout>